{
    "name": "erut-web",
    "version": "0.0.0",
    "description": "erupt front-end",
    "author": "yuepeng <erupts@126.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erupts/erupt-web.git"
    },
    "bugs": {
        "url": "https://github.com/erupts/erupt-web/issues"
    },
    "homepage": "https://www.erupt.xyz",
    "license": "Apache-2.0",
    "keywords": [
        "erupt",
        "yuepeng",
        "antd",
        "ng-zorro-antd",
        "ng-alain",
        "angular"
    ],
    "scripts": {
        "ng-high-memory": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng",
        "ng": "ng",
        "start": "ng s -o",
        "build": "npm run ng-high-memory build",
        "analyze": "npm run ng-high-memory build -- --source-map",
        "analyze:view": "source-map-explorer dist/**/*.js",
        "color-less": "ng-alain-plugin-theme -t=colorLess",
        "theme": "ng-alain-plugin-theme -t=themeCss",
        "icon": "ng g ng-alain:plugin icon"
    },
    "dependencies": {
        "@angular/animations": "^19.0.0",
        "@angular/common": "^19.0.0",
        "@angular/compiler": "^19.0.0",
        "@angular/core": "^19.0.0",
        "@angular/forms": "^19.0.0",
        "@angular/platform-browser": "^19.0.0",
        "@angular/platform-browser-dynamic": "^19.0.0",
        "@angular/router": "^19.0.0",
        "@angular/cdk": "^19.0.0",
        "@antv/g2plot": "^2.4.32",
        "@ctrl/tinycolor": "^4.1.0",
        "@delon/abc": "^19.0.0",
        "@delon/auth": "^19.0.0",
        "@delon/cache": "^19.0.0",
        "@delon/form": "^19.0.0",
        "@delon/theme": "^19.0.0",
        "@delon/util": "^19.0.0",
        "@micro-zoe/micro-app": "^1.0.0-rc.19",
        "@ng-util/lazy": "^14.0.0",
        "@ng-util/util": "^14.0.0",
        "@panzoom/panzoom": "^4.6.0",
        "@worktile/gantt": "^19.0.0",
        "@date-fns/tz": "^1.0.0",
        "angular-gridster2": "^19.0.0",
        "date-fns": "^4.0.0",
        "eslint": "^9.0.0",
        "font-awesome": "^4.7.0",
        "html2canvas": "^1.4.1",
        "intl": "^1.2.5",
        "lodash": "~4.17.21",
        "moment": "^2.29.4",
        "monaco-editor": "^0.34.1",
        "ng-zorro-antd": "^19.0.0",
        "rxjs": "~7.8.0",
        "screenfull": "^6.0.2",
        "signature_pad": "^5.0.10",
        "ts-md5": "^1.3.1",
        "tslib": "^2.3.0",
        "vditor": "^3.11.1",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-eslint/schematics": "^19.0.0",
        "@angular-eslint/template-parser": "^19.0.0",
        "@angular/build": "^19.0.0",
        "@angular/cli": "~19.0.0",
        "@angular/compiler-cli": "^19.0.0",
        "@angular/language-service": "^19.0.0",
        "@types/lodash": "~4.17.15",
        "@types/node": "^20.0.0",
        "@typescript-eslint/utils": "^8.0.0",
        "@typescript-eslint/types": "^8.0.0",
        "less": "^4.2.0",
        "node-fetch": "^2.6.1",
        "source-map-explorer": "^2.5.2",
        "ts-node": "~10.9.1",
        "typescript": "~5.7.0",
        "vite": "^7.3.0",
        "vite-plugin-static-copy": "^3.1.4"
    },
    "lint-staged": {
        "(src)/**/*.{html,ts}": [
            "eslint --fix"
        ],
        "(src)/**/*.less": [
            "npm run lint:style"
        ]
    }
}
